{
  "expo": {
    "name": "The Little Gardener",
    "slug": "thelittlegardener",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tlglogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.test.thelittlegardener"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tlglogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.thelittlegardener"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/tlglogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tlglogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to allow you to select an image for analysis."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
